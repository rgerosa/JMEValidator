{
    "analysis_modules": [
        "ProjectByFit"
    ], 
    "filename" : "Longitudinal_Resolution_NPV",
    "fit_backend": [
        "ROOT"
    ], 
    "line_styles": [
        "-", "--"
    ], 
    "markers": [
        "."
    ], 
    "projection_function": [
        "gaus"
    ], 
    "projection_parameters": [
        "1000,10,10"
    ], 
    "projection_to_nick": [
        "nick0", 
        "nick1", 
        "nick2",
        "nick3",
        "nick4",
        "nick5",
        "nick6", "nick7", "nick8", "nick9"
    ], 
    "projection_to_plot": [
        "2"
    ], 
    "weights": [
        "Boson_daughter==13&&Boson_Pt>30", "z_pT>30"
    ], 
    "x_bins": [
        "7,5,30"
    ], 
    "x_expressions": [
        "NVertex", "nPV"
    ], 
    "x_label": "NPV", 
    "y_bins": [
        "50,-100,100"
    ], 
    "y_expressions": [
        "recoilslimmedMETs_LongZ+Boson_Pt",             "particleFlowUpdate_Upar+z_pT", 
        "recoilpatpfTrackMET_LongZ+Boson_Pt",        "trackUpdate_Upar+z_pT",
        "recoilpatpfNoPUMET_LongZ+Boson_Pt",         "noPileUpUpdate_Upar+z_pT",
        "recoilpatpfPUMET_LongZ+Boson_Pt",           "pileUpUpdate_Upar+z_pT",
        "recoilslimmedMETsPuppi_LongZ+Boson_Pt",     "0"
    ], 
    "y_label": "Resolution [GeV]",
	"y_lims" : [0, 50]
}
